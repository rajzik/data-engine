{
  "name": "data-engine",
  "version": "1.0.0",
  "description": "Data processing, filter and sort",
  "scripts": {
    "test:node": "jest",
    "test": "npm run test:node",
    "build:demo": "cross-env NODE_ENV=production webpack -p --optimize-dedupe",
    "build": "npm run check-version && node scripts/rollup/build.js",
    "start": "webpack-dev-server",
    "check-version": "node ./scripts/prerelease/check-version.js"
  },
  "keywords": [
    "Sort",
    "filter",
    "data"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "*",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "*",
    "babel-runtime": "*",
    "clean-webpack-plugin": "*",
    "cross-env": "*",
    "expose-loader": "*",
    "fs": "0.0.1-security",
    "glob": "^7.1.1",
    "html-webpack-plugin": "*",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "*",
    "jest": "*",
    "json-loader": "*",
    "ncp": "^2.0.0",
    "nyan-progress-webpack-plugin": "*",
    "raw-loader": "*",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.2",
    "tar.gz": "^1.0.5",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "author": "Jan Silhan",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rajzik/data-engine.git"
  },
  "jest": {
    "moduleNameMapper": {
      "^filter-value$": "<rootDir>/src/data-helpers/filter-helpers/filter-value.js",
      "^data-filter$": "<rootDir>/src/data-helpers/data-filter.js",
      "^data-sort$": "<rootDir>/src/data-helpers/data-sort.js"
    },
    "transform": {
      ".js": "<rootDir>/node_modules/babel-jest"
    },
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
